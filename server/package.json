{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon --inspect ./bin/www",
    "web-app": "npm start --prefix ../web-app",
    "web-admin": "npm start --prefix ../web-admin",
    "development-client": "concurrently \"npm run server\" \"npm run web-app\"",
    "development-admin": "concurrently \"npm run server\" \"npm run web-admin\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.81.0",
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.3.0",
    "concurrently": "^6.5.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.3.1",
    "express-mysql-session": "^2.1.7",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "sequelize": "^6.12.0-alpha.1",
    "sharp": "^0.30.4",
    "validate.js": "^0.13.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-loggly-bulk": "^3.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "chai": "^4.3.4",
    "faker": "^5.5.3",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0",
    "sinon": "^12.0.1"
  }
}
